// Write a program to apply left shift and right shift operator.
#include <stdio.h>
int main(){

int a;
int b;

printf("enter the value of a\n");
scanf("%d",&a);
printf("enter the value of b\n");
scanf("%d",&b);

printf("left shift of a-> %d\n", a<<1);
printf("right shift of a-> %d\n",a>>1);

printf("left shift of b-> %d\n", b<<1);
printf("right shift of b-> %d\n",b>>1);


}
