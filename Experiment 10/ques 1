// Write a program to create a simple linked list in C using pointer and structure.
#include <stdio.h>

struct list {
   
     int age;
     int marks;
  
};

int main() {
    struct list l1,l2,l3;

    l1.age=19;
    l1.marks=95;
    l2.age=45;
    l2.marks=85;
    l3.age=23;
    l3.marks=89;

   printf("age of first person-> %d\n",l1.age);
  
   printf("age of second person-> %d\n",l2.age);

   printf("age of third person-> %d\n", l3.age);

   printf("marks of first person-> %d\n",l1.marks);
  
   printf("marks of second person-> %d\n",l2.marks);

   printf("marks of third person-> %d\n", l3.marks);

    return 0;
}
